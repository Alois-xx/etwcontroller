<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="ETWControler.Configuration" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
        </sectionGroup>
        <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
            <section name="ETWControler.Configuration" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
        </sectionGroup>
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
    </startup>
    <userSettings>
        <ETWControler.Configuration>
            <setting name="PortNumber" serializeAs="String">
                <value>4295</value>
            </setting>
            <setting name="Host" serializeAs="String">
                <value>localhost</value>
            </setting>
            <setting name="WCFPort" serializeAs="String">
                <value>8091</value>
            </setting>
            <setting name="SlowEventMessage" serializeAs="String">
                <value>Here it was slow</value>
            </setting>
            <setting name="SlowEventHotkey" serializeAs="String">
                <value>Insert</value>
            </setting>
            <setting name="LocalTraceStart" serializeAs="String">
                <value>-start GeneralProfile</value>
            </setting>
            <setting name="LocalTraceStop" serializeAs="String">
                <value>-stop $FileName</value>
            </setting>
            <setting name="ServerTraceStart" serializeAs="String">
                <value>-start GeneralProfile</value>
            </setting>
            <setting name="ServerTraceStop" serializeAs="String">
                <value>-stop $FileName</value>
            </setting>
            <setting name="LocalTraceEnabled" serializeAs="String">
                <value>True</value>
            </setting>
            <setting name="ServerTraceEnabled" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="FastEventMessage" serializeAs="String">
                <value>Here it was fast again</value>
            </setting>
            <setting name="FastEventHotkey" serializeAs="String">
                <value>Snapshot</value>
            </setting>
            <setting name="ScreenshotDirectory" serializeAs="String">
                <value>%temp%\ETWControlerScreenshots</value>
            </setting>
            <setting name="LocalTraceCancel" serializeAs="String">
                <value>-cancel</value>
            </setting>
            <setting name="ServerTraceCancel" serializeAs="String">
                <value>-cancel</value>
            </setting>
            <setting name="TraceFileName" serializeAs="String">
                <value>c:\temp\etwControler%COMPUTERNAME%.etl</value>
            </setting>
            <setting name="ForcedScreenshotIntervalinMs" serializeAs="String">
                <value>2000</value>
            </setting>
        </ETWControler.Configuration>
    </userSettings>
    <applicationSettings>
        <ETWControler.Configuration>
            <setting name="Presets" serializeAs="Xml">
                <value>
                    <ArrayOfPreset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                        xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                        <Preset>
                            <Name>Default</Name>
                            <TraceStartCommand>-start GeneralProfile</TraceStartCommand>
                            <TraceStopCommand>-stop $FileName</TraceStopCommand>
                            <TraceCancelCommand>-cancel</TraceCancelCommand>
                        </Preset>
                        <Preset>
                            <Name>Default and .NET</Name>
                            <TraceStartCommand>-start GeneralProfile -start DotNET</TraceStartCommand>
                            <TraceStopCommand>-stop $FileName</TraceStopCommand>
                            <TraceCancelCommand>-cancel</TraceCancelCommand>
                        </Preset>
                    </ArrayOfPreset>
                </value>
            </setting>
        </ETWControler.Configuration>
    </applicationSettings>
</configuration>
